<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application>
      <lifetime leaseTime="10M" sponsorshipTimeout="10M" renewOnCallTime="10M" leaseManagerPollTime="1M"/>

      <channels>
        <channel type="Belikov.GenuineChannels.GenuineTcp.GenuineTcpChannel, HS2Util"/>
      </channels>
    </application>
  </system.runtime.remoting>
  <appSettings>
    <add key="ServiceURL" value="tcp://127.0.0.1:8090/Service.TServiceHelper"/>

    <add key="ID1" value="7"/>
    <add key="ID2" value="8"/>
    <add key="ID3" value="9"/>
    <add key="ID4" value="10"/>
    <add key="ID5" value="11"/>
    <add key="ID6" value="12"/>
    <add key="ID7" value="13"/>
    <add key="ID8" value="14"/>

    <add key="TimeOut" value="20"/>
    <add key="MaxRow" value="100"/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="DLL;LIB"/>
    </assemblyBinding>
    <generatePublisherEvidence enabled="false"/>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
